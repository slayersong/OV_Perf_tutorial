#usda 1.0

def Material "mat_1" {
    token outputs:surface.connect = </mat_1/shader.outputs:surface>

    def Shader "shader" {
        uniform token info:implementationSource = "sourceAsset"
        uniform asset info:sourceAsset = @asset.txt@
        token outputs:surface
    }
}

